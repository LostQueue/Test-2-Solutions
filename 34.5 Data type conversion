# Get five integer inputs from the user
input_values = []
for i in range(5):
    value = int(input(f"Enter input {i + 1}: "))
    input_values.append(value)

# Calculate the sums in different data types
int_sum = sum(input_values)  # Integer sum
float_sum = float(int_sum)  # Float sum
str_sum = "".join(map(str, input_values))  # String sum

# Print the results
print(f"Integer: {int_sum}")
print(f"Float: {float_sum}")
print(f"String: {str_sum}")
